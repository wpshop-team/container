{
  "name": "wpshop/container",
  "type": "library",
  "description": "PHP Container",
  "keywords": [
    "dependency injection",
    "container"
  ],
  "require": {
    "php": ">=7.2.5",
    "psr/container": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "WPShop\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WPShopTest\\Container\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
